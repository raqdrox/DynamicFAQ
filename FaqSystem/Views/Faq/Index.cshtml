@model Tuple<IEnumerable<FaqSystem.Models.FaqSection>,FaqQuestion>

@{
    ViewData["Title"] = "FAQ";
}
<html>

<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="/js/jquery-3.6.0.min.js"></script>
    <script src="/bootstrap/js/bootstrap.min.js"></script>

    <script defer src="/fontawesome-5.15.4/js/solid.js"></script>
    <script defer src="/fontawesome-5.15.4/js/fontawesome.min.js"></script>
    <script src="/js/popper.min.js"></script>
    <link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/css/Faq.css" />
</head>
<body>

    <div class="wrapper">

        <!-- Sidebar  -->
        <partial name="_SidebarPartial" model="@Model.Item1" />

        
        <!-- Page Content  -->
        <partial name="_ArticlePartial" model="@Model" />

    </div>

</body>


</html>



